  "compilerOptions": {
    "target": "es5",
    "module": "esNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "lib": ["es7", "dom", "dom.iterable", "scripthost"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
